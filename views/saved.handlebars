<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <a class="navbar-brand main-nav" href="#">GitWorkd</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav ml-auto">
          <a class="nav-link" href="/main"><i class="fas fa-comment-alt"></i> Feed</a>
          <a class="nav-link" href="/savedRepos"><i class="fas fa-archive"></i> Saved Repos</a>
          <a class="nav-link" href="/addRepo"><i class="fas fa-plus-square"></i> Add Repos</a>
