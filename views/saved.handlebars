      <div class="container">
        <div class="row">
          <div class="col-sm-8 mx-auto mt-5">
            <table class="table table-hover table-dark table-sm">
              <thead>
                <tr>
                  <th scope="col"><i class="fas fa-book-open ml-3"></i> Title</th>
                  <th scope="col"><i class="fab fa-github mr-1"></i>Author</th>
                  <th scope="col"><i class="fas fa-eye"></i> View</th>
                </tr>
              </thead>
              <tbody>
                {{#each repos}}
                <tr>
                  <td>{{name}}</td>
                  <td>{{author}}</td>
                  <td><a href="{{url}}" class="btn btn-sm btn-outline-light">View Repository</a></td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
        </div>
      </div>

  